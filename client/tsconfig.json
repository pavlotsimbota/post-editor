{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "noImplicitAny": false,
    "baseUrl": ".",
    "outDir": "./build",
    "jsx": "react"
  },
  "exclude": [
    "./node_modules"
  ]
}
